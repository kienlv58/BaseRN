{
  "name": "obytesapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "husky install",
    "pre-commit": "lint-staged",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "clean": "react-native-clean-project",
    "setup:ios": "cd ios && pod install --repo-update && cd ..",
    "np": "np --no-publish",
    "postversion": "react-native-version",
    "xcode": "xed -b ios",
    "commit": "cz"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.8.8",
    "@react-native-async-storage/async-storage": "^1.17.7",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/bottom-tabs": "^6.3.1",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/stack": "^6.2.1",
    "@reduxjs/toolkit": "^1.8.2",
    "@shopify/restyle": "^2.1.0",
    "axios": "^0.25.0",
    "i18n-js": "^3.9.2",
    "lodash.memoize": "^4.1.2",
    "react": "17.0.2",
    "react-error-boundary": "^3.1.3",
    "react-hook-form": "^7.29.0",
    "react-native": "0.68.1",
    "react-native-bootsplash": "^4.1.4",
    "react-native-dotenv": "^3.3.1",
    "react-native-exception-handler": "^2.10.10",
    "react-native-flash-message": "^0.2.1",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-localize": "^2.2.1",
    "react-native-mmkv": "^2.3.1",
    "react-native-reanimated": "^2.10.0",
    "react-native-safe-area-context": "^3.4.1",
    "react-native-screens": "^3.13.1",
    "react-native-svg": "^12.3.0",
    "react-query": "^3.34.19",
    "react-redux": "^8.0.2",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.1",
    "yup": "^0.32.11",
    "zustand": "^3.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@bam.tech/react-native-make": "^3.0.0",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@react-native-community/eslint-config": "^3.0.1",
    "@types/jest": "^26.0.23",
    "@types/lodash.memoize": "^4.1.6",
    "@types/react-native": "^0.67.3",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "husky": "^8.0.1",
    "jest": "^26.6.3",
    "lint-staged": "^13.0.2",
    "metro-react-native-babel-preset": "^0.67.0",
    "np": "^7.6.1",
    "npm-run-all": "^4.1.5",
    "react-native-clean-project": "^4.0.1",
    "react-native-flipper": "^0.151.0",
    "react-native-version": "^4.0.0",
    "react-test-renderer": "17.0.2",
    "redux-flipper": "^2.0.1",
    "typescript": "4.4.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --ignore-path .gitignore --ignore-pattern static/ --ext .js,.ts,.tsx --fix",
      "prettier --write",
      "jest --passWithNoTests"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/username/your-repo.git"
  }
}
